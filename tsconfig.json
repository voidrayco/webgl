{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "importHelpers": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "target": "es2017",
    "rootDirs": [
      "node_modules"
    ]
  }
}
